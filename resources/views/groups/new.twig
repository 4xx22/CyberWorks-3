                        {% if auth.permissions.can_edit_group_perms_player %}
                            <li role="presentation" class="active">
                                <a href="#player_perms" id="player_perms-tab" role="tab" data-toggle="tab" aria-expanded="true">Player</a>
                            </li>
                        {% endif %}
                        {% if auth.permissions.can_edit_group_perms_vehicle %}
                            <li role="presentation">
                                <a href="#vehicle_perms" id="vehicle_perms-tab" role="tab" data-toggle="tab" aria-expanded="true">Vehicle</a>
                            </li>
                        {% endif %}
                        {% if auth.permissions.can_edit_group_perms_gang %}
