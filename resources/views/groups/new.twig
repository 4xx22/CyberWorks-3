{% extends 'templates/template.twig' %}

{% block title %}
    | New Group
{% endblock %}

{% block style %}
    <link href="{{ base_url() }}/assets/vendors/switchery/dist/switchery.min.css" rel="stylesheet" />
{% endblock %}

{% block content %}
<form class="form-horizontal form-label-left" method="post" action="{{ path_for('group.new') }}">
    <div class="row">
        <div class="col-md-3 col-sm-3 col-xs-3">
            <div class="x_panel">
                <div class="x_title">
                    <h2>New Group</h2>
                    <div class="clearfix"></div>
                </div>
                <div class="x_content">
                    <div class="form-group">
                        <label class="col-sm-3 control-label"> Title</label>
                        <div class="col-sm-9">
                            <div class="input-group">
                                {% if auth.permissions.can_edit_group_name %}
                                    <input type="text" class="form-control" name="group_name">
                                {% else %}
                                    <input type="text" class="form-control" name="group_name" disabled>
                                {% endif %}
                            </div>

                        </div>
                    </div>
                    {% if useIps %}
                        <div class="form-group">
                            <label class="col-sm-3 control-label"> IPS ID</label>
                            <div class="col-sm-9">
                                <div class="input-group">
                                    {% if auth.permissions.can_edit_group_ips_id %}
                                        <input type="number" class="form-control" name="group_id">
                                    {% else %}
                                        <input type="text" class="form-control" name="group_id" disabled>
                                    {% endif %}
                                </div>
                            </div>
                        </div>
                    {% endif %}

                    <button type="submit" class="btn btn-warning" style="display: block; width:100%;"  data-toggle="modal" data-target=".compensate">Save</button>

                    <div class="clearfix"></div>
                </div>
            </div>
        </div>
        <div class="col-md-9 col-sm-9 col-xs-9">
            <div class="row">
                <h1 class="text-center">Group Permissions</h1>
                <div class="" role="tabpanel" data-example-id="togglable-tabs">
